{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [
      "warn",
      "always"
    ]
  }
}
